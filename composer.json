{
	"name" : "synthmedia/notification",
	"description" : "A bundle to provide a notification system, much like that of Facebook (but a basic version at present) ",
	"license" : "BSD license",
	"type" : "symfony-bundle",
	"require" : {
		"symfony/symfony" : "~2.5",
		"symfony/routing" : "~2.5",
		"symfony/framework-bundle" : "~2.5",
		"symfony/swiftmailer-bundle" : "~2.3",
		"doctrine/orm" : "~2.3"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4.3"
	},
	"config" : {
		"bin-dir" : "bin/",
		"notify-on-install" : false,
		"github-protocols" : [
			"https",
			"git",
			"ssh"
		]
	}
}